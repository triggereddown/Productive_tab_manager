{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Organize your open tabs and track and remove them for productivity and session saving.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
